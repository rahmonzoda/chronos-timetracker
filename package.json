{
  "name": "Chronos",
  "version": "2.7.1",
  "description": "Full functionality time tracking software with direct JIRA integration",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --progress --profile --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max_old_space_size=8192 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --progress --profile --colors",
    "dev": "cross-env UPLOAD_SENTRY=0 START_HOT=1 yarn start-renderer-dev",
    "flow": "flow check",
    "flow-typed": "rm -rf flow-typed/yarn && flow-typed install --overwrite || true",
    "flow:watch": "flow-watch",
    "lint": "eslint ./app --ext .js,.jsx --ignore-pattern '/dist/' --ignore-pattern '*.prod.js' --ignore-pattern '**/node_modules/*' --ignore-pattern '*.min.js'",
    "lint-fix": "yarn lint -- --fix",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "prestart": "npm run build",
    "postinstall": "concurrently \"yarn build-dll\" \"electron-builder install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\" \"yarn flow-typed\"",
    "package": " cross-env UPLOAD_SENTRY=1 yarn build && build --publish never",
    "package-dev": " cross-env DEBUG_PROD=false UPLOAD_SENTRY=0 yarn build && build --publish never",
    "package-release": "yarn build && build -p always",
    "package-win": "yarn build && build --win --x64",
    "package-linux": "yarn build && build --linux",
    "package-all": "yarn build && build -mwl",
    "commit": "git-cz",
    "release": "sh ./scripts/release.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -k ./app/package.json"
  },
  "author": {
    "name": "Webpal",
    "url": "https://github.com/web-pal"
  },
  "license": "ISC",
  "build": {
    "productName": "Chronos",
    "appId": "com.web-pal.chronos",
    "copyright": "Copyright Â© 2017 Webpal",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "idlePopup.html",
      "screenPopup.html",
      "issueForm.html",
      "assets/images/icon.png",
      "assets/images/icon-active.png",
      "preload.js",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "asarUnpack": "./node_modules/node-notifier/vendor/**",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Clock"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web-pal/chronos-timetracker.git"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "commitizen": "2.9.6",
    "concurrently": "3.5.1",
    "conventional-changelog-cli": "1.3.8",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "cz-customizable": "5.2.0",
    "devtron": "1.4.0",
    "electron": "1.7.12",
    "electron-builder": "19.55.3",
    "electron-debug": "1.5.0",
    "electron-devtools-installer": "2.2.3",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-flowtype-errors": "3.3.7",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "fbjs-scripts": "0.8.1",
    "file-loader": "1.1.6",
    "flow-bin": "0.68.0",
    "flow-typed": "2.4.0",
    "flow-watch": "1.1.3",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "react-hot-loader": "3.1.3",
    "redux-logger": "3.0.6",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1",
    "webpack-sentry-plugin": "1.14.1"
  },
  "dependencies": {
    "@atlaskit/badge": "6.3.1",
    "@atlaskit/button": "6.6.3",
    "@atlaskit/calendar": "^4.0.0",
    "@atlaskit/checkbox": "1.4.0",
    "@atlaskit/dropdown-menu": "3.12.1",
    "@atlaskit/field-base": "8.2.1",
    "@atlaskit/field-radio-group": "1.9.2",
    "@atlaskit/field-text": "^4.3.0",
    "@atlaskit/field-text-area": "1.2.2",
    "@atlaskit/flag": "6.5.1",
    "@atlaskit/icon": "10.13.0",
    "@atlaskit/inline-dialog": "5.3.2",
    "@atlaskit/inline-edit": "4.6.3",
    "@atlaskit/input": "1.7.1",
    "@atlaskit/lozenge": "3.6.1",
    "@atlaskit/modal-dialog": "3.5.1",
    "@atlaskit/single-select": "3.3.3",
    "@atlaskit/spinner": "4.2.1",
    "@atlaskit/tag": "3.2.1",
    "@atlaskit/tooltip": "8.4.1",
    "bufferutil": "3.0.3",
    "calculate-size": "1.1.1",
    "electron-json-storage": "4.0.2",
    "electron-log": "2.2.14",
    "electron-updater": "2.20.1",
    "font-awesome": "4.7.0",
    "jira-connector": "github:web-pal/jira-connector#290772714e5ad00aa34854dcc5cd22f6a9c4ce43",
    "keytar": "^4.1.0",
    "merge-images": "1.0.7",
    "minimist": "^1.2.0",
    "mixpanel-browser": "2.17.0",
    "moment": "2.20.1",
    "moment-duration-format": "2.2.1",
    "nanotimer": "0.3.15",
    "normalizr": "3.2.4",
    "ramda": "0.25.0",
    "raven-js": "3.22.1",
    "rc-time-picker": "3.2.1",
    "react": "16.2.0",
    "react-click-outside": "github:tj/react-click-outside",
    "react-dom": "16.2.0",
    "react-markdown": "3.1.5",
    "react-redux": "5.0.7",
    "react-stickynode": "^1.4.1",
    "react-transition-group": "2.2.1",
    "react-virtualized": "9.18.5",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-form": "7.2.3",
    "redux-resource": "2.4.0",
    "redux-resource-action-creators": "1.0.0",
    "redux-resource-plugins": "2.1.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "smoothscroll-polyfill": "^0.4.0",
    "socket.io-client": "2.0.1",
    "source-map-support": "0.5.3",
    "styled-components": "1.4.6 - 3",
    "utf-8-validate": "4.0.0"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": "0.22.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
